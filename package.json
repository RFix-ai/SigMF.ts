{
  "name": "sigmf",
  "version": "0.1.0",
  "description": "TypeScript library for reading and writing SigMF (Signal Metadata Format) files",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sigmf",
    "signal",
    "metadata",
    "sdr",
    "radio",
    "iq",
    "samples",
    "rf"
  ],
  "author": "Bar Harel <bar@barharel.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RFix-ai/SigMF.ts.git"
  },
  "bugs": {
    "url": "https://github.com/RFix-ai/SigMF.ts/issues"
  },
  "homepage": "https://github.com/RFix-ai/SigMF.ts#readme",
  "dependencies": {
    "tarts": "^2.0.1"
  }
}
